<div class="col-xs-12">
	<div class="meme-top">
		<h3><a class="meme-title" href="#memes/<%= section%>/<%= _id%>"><%= title%></a></h3>
	</div>
	<div class="meme-body">
		<a href="#memes/<%= section%>/<%= _id%>">
			<%  var arr = link.split('.'); arr[arr.length-2] += "l"; var img_url = arr.join('.'); %>
			<img class="meme-image" src="<%= img_url%>">
		</a>
	</div>
	<div class="meme-bottom">
		<p>Something here</p>
	</div>
</div>
